@using InventoryManagementSystem.Models.Product
@model IEnumerable<Product>

<table>
    <tr>
        <th>Name</th>
        <th>Quantity</th>
        <th>Description</th>
        <th>Price</th>
    </tr>
    @foreach (var item in Model) 
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Quantity</td>
            <td>@item.Description</td>
            <td>@item.Price</td>
        </tr>
    }
</table>